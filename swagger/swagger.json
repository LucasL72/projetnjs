 <a class=\"nav-link\" href=\"/blog\">Blog</a>\n                </li>\n                <li class=\"nav-item\">\n                    <a class=\"nav-link\" href=\"/contact\">Nous contacter</a>\n                </li>\n            </ul>\n\n            <!-- 2 Modal lost password -->\n            \n            <div class=\"modal fade\" id=\"lostpassword\" aria-hidden=\"true\" aria-labelledby=\"ModalToggleLabel2\" tabindex=\"-1\">\n                <div class=\"modal-dialog modal-dialog-centered\">\n                    <div class=\"modal-content\">\n                        <div class=\"modal-header\">\n                            <h5 class=\"modal-title\" id=\"ModalToggleLabel2\">Mot de passe oublié</h5>\n                            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                        </div>\n                        <div class=\"modal-body\">\n                            <form action=\"/lostpassword\" method=\"post\">\n                                <div class=\"mb-3\">\n                                    <p class=\"fst-italic\"> Nous vous enverrons le mail de réinitialisation</p>\n                                    <input name=\"author\" class=\"form-control\" type=\"name\" placeholder=\"Votre Nom\"> \n                                    <input name=\"email\" type=\"email\" class=\"form-control\" id=\"loginEmail\"\n                                        aria-describedby=\"emailHelp\" placeholder=\"Votre Adresse Mail\">\n                                    <div id=\"emailHelp\" class=\"form-text\"></div>\n                                    <button type=\"submit\" class=\"btn btn-primary\">Envoyer</button>\n                                </div>\n                                <div class=\"modal-footer\">\n                                    <button type=\"submit\" class=\"btn btn-contact\" data-bs-target=\"#connectModal\" data-bs-toggle=\"modal\">Se\n                                        connecter</button>\n                                </div>\n                            </form>\n                        </div>\n                    </div>\n                </div>\n            </div>            <button type=\"button\" class=\"btn btn-full\" data-bs-toggle=\"modal\" data-bs-target=\"#connectModal\">\n                Se connecter/S'inscrire\n            </button>\n            <!--  Modal connect -->\n            \n            \n            <div class=\"modal fade\" id=\"connectModal\" tabindex=\"-1\" aria-labelledby=\"ModalLabel\" aria-hidden=\"true\">\n                <div class=\"modal-dialog\">\n                    <div class=\"modal-content\">\n                        <div class=\"modal-header\">\n                            <h5 class=\"modal-title\" id=\"ModalLabel\">Se connecter/S'inscrire</h5>\n                            <button type=\"submit\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                        </div>\n                        <div class=\"modal-body\">\n                            <form action=\"/login\" method=\"post\">\n                                <div class=\"container\">\n                                    <div class=\"mb-3\">\n                                        <label for=\"loginEmail\" class=\"form-label\">Adresse Mail</label>\n                                        <input name=\"email\" type=\"email\" class=\"form-control\" id=\"loginEmail\"\n                                            aria-describedby=\"emailHelp\">\n                                    </div>\n                                    <div class=\"mb-3\">\n                                        <label for=\"Password\" class=\"form-label\">Mot de passe</label>\n                                        <input name=\"password\" type=\"password\" class=\"form-control\" id=\"Password\">\n                                    </div>\n            \n                                    <a data-bs-toggle=\"modal\" href=\"#lostpassword\" role=\"button\">Mot de passe\n                                        oublié</a><br>\n                                    <button type=\"submit\" class=\"btn btn-contact\">Se connecter</button>\n                                    <a href=\"register\" class=\"btn btn-ghost\">Nouvel Utilisateur</a>\n                                </div>\n                            </form>\n            \n                        </div>\n                        <div class=\"modal-footer\">\n                            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Fermer</button>\n            \n                        </div>\n                    </div>\n                </div>\n            </div>\n\n\n        </div>\n    </div>\n\n</nav>\n<header>\n    <div class=\"container\">\n        <div class=\"row home-title\">\n            <div class=\"col-md-12\">\n                 <h1>Bienvenue</h1>\n                <div class=\"typewriter-container\">\n                    <div class=\"typewriter\">\n                        <p class=\"welcome\">Rejoignez l'aventure!</p>\n                    </div>\n                </div>\n\n            </div>\n        </div>\n    </div>\n\n</header>\n<!--- Cards-->\n<section id=\"cards\">\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-md-12\">\n                <h2> Actualités</h2>\n\n            </div>\n           \n        </div>\n\n</section>    \n\n<!--- bannière--->\n    <section class=\"banniere\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-md-12\">\n                    <h2>La bannière</h2>\n                    <h3>\"Avec du temps et de la patience, on vient à bout de tout.\"</h3>\n                </div>\n            </div>\n        </div>\n    </section>\n<!--- Contact--->\n\n<section class=\"theme-contact\">\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-md-12\">\n          